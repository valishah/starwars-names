{
  "name": "starwars--names",
  "version": "0.0.0-development",
  "description": "webpack configuration",
  "main": "src/index.js",
  "scripts": {
    "precommit": "npm run test:single && npm run check-coverage",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Vali Shah <vali.shah7@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.0.2",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "husky": "0.14.2",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "semantic-release": "^6.3.6"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valishah/starwars-names.git"
  },
  "bugs": {
    "url": "https://github.com/valishah/starwars-names/issues"
  },
  "homepage": "https://github.com/valishah/starwars-names#readme"
}
